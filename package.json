{
  "name": "vscode-grep",
  "displayName": "Grep Search",
  "description": "A VS Code extension that provides grep-like search functionality. Search within the currently active file using grep patterns and parameters.",
  "version": "0.0.1",
  "publisher": "CYYoung88",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/CYyoung88/vscode-grep.git"
  },
  "icon": "icon.png",
  "keywords": [
    "grep",
    "search",
    "find",
    "pattern",
    "regex"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:grep.search"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "grep.search",
        "title": "Grep"
      }
    ],
    "languages": [
      {
        "id": "grep-result",
        "aliases": [
          "Grep Result"
        ],
        "extensions": [
          ".grepresult"
        ]
      }
    ],
    "grammars": [
      {
        "language": "grep-result",
        "scopeName": "text.grep.result",
        "path": "./syntaxes/grep-result.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "npx tsc -p ./",
    "watch": "npx tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "sharp": "^0.34.5",
    "typescript": "^4.9.4"
  }
}
